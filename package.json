{
  "name": "qfang-starter-kit",
  "version": "0.0.2",
  "author": "Victor Lin",
  "scripts": {
    "clear": "rm -rf .happypack dist",
    "start": "npm run clear && cross-env NODE_ENV=development webpack-dev-server --hot",
    "build": "npm run clear && cross-env NODE_ENV=production webpack --optimize-minimize --progress --profile",
    "deploy:test": "npm run clear && cross-env NODE_ENV=test webpack --config webpack.deploy.js --optimize-minimize --progress --profile"
  },
  "devDependencies": {
    "coala": "~0.0.8",
    "copy-webpack-plugin": "^4.0.0",
    "cross-env": "^3.2.3",
    "css-loader": "^0.26.1",
    "dot-loader": "^0.1.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-plugin-import": "^2.2.0",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "0.8.4",
    "glob": "^7.1.1",
    "happypack": "^3.0.3",
    "html-webpack-plugin": "^2.24.0",
    "jquery": "^1.12.3",
    "ssh-webpack-plugin": "^0.1.7",
    "style-loader": "^0.12.4",
    "url-loader": "^0.5.6",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1",
    "webpack-md5-hash": "^0.0.5"
  },
  "keywords": [
    "coala",
    "qfang"
  ],
  "license": "MIT",
  "repository": {
    "type": "git"
  }
}
